{
  "name": "Lazy Tabs",
  "version": "0.0.0.0",
  "manifest_version": 2,
  "description": "Discard all inactive tabs (except pinned tabs) at Google Chrome startup",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "require(./ico_128.png)",
    "48": "require(./ico_48.png)"
  }
}